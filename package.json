{
  "name": "es-drager-vue2",
  "version": "0.0.1",
  "description": "A draggable, resizable, rotatable component based on vue3",
  "keywords": [
    "drag",
    "resize",
    "rotate",
    "vue-drager",
    "vue-drag-resize",
    "vue-drager",
    "es-drager"
  ],
  "homepage": "https://github.com/vangleer/es-drager",
  "bugs": {
    "url": "https://github.com/vangleer/es-drager/issues"
  },
  "license": "MIT",
  "author": "huangwantong <965002107@qq.com>",
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/drager/index.d.ts"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vangleer/es-drager.git"
  },
  "scripts": {
    "dev": "npm run dev -w docs",
    "build": "vite build",
    "build:docs": "npm run build -w docs",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write docs/**/*.{vue,ts,tsx,json}"
  },
  "workspaces": [
    "docs"
  ],
  "dependencies": {
    "es-drager": "^1.2.1",
    "vue": ">=3.2.0",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "prettier": "3.0.3",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "typings": "lib/drager/index.d.ts"
}
